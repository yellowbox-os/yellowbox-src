#!/bin/sh

# Source GNUstep environment
. /System/Library/Makefiles/GNUstep.sh

# Start XFCE settings daemon
xfsettingsd &

# Start clipboard manager
xfce4-clipman &

# Start XFWM4 (window manager)
xfwm4 & sleep 1 &&

# Start Panel
xfce4-panel & sleep 1 &&

# Start desktop
xfdesktop & sleep 1 &&

# Start GNUstep Workspace
openapp Workspace
